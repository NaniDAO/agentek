{
  "name": "agentek",
  "description": "A collection of AI agent tools for blockchain interaction",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/ai-sdk",
    "packages/shared",
    "packages/mcp"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
