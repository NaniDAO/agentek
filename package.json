    "clean": "rm -rf dist src/shared/dist src/ai-sdk/dist src/mcp/dist"
  },
  "keywords": [
    "ai",
    "agents",
    "blockchain",
    "ethereum",
    "tools"
  ],
  "author": "NaniDAO",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NaniDAO/agentek.git"
  },
  "homepage": "https://nani.ooo",
  "devDependencies": {
    "@openrouter/ai-sdk-provider": "^0.0.6",
    "@types/node": "^22.10.6",
    "dotenv": "^16.4.7",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}