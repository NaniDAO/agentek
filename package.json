{
  "name": "agentek",
  "version": "0.1.0",
  "description": "A collection of AI agent tools for blockchain interaction",
  "type": "module",
  "private": true,
  "workspaces": [
    "src/ai-sdk",
    "src/shared",
    "src/mcp"
  ],
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
