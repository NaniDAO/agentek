{
  "name": "@agentek/mcp-server",
  "version": "0.1.21",
  "description": "Model Context Protocol (MCP) server for Agentek",
  "license": "AGPL-3.0",
  "author": "NaniDAO",
  "homepage": "https://nani.ooo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NaniDAO/agentek.git"
  },
  "type": "module",
  "bin": {
    "@agentek/mcp-server": "dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.mjs",
    "test": "vitest run",
    "debug": "pnpm run build && pnpx @modelcontextprotocol/inspector node ./dist/index.mjs"
  },
  "dependencies": {
    "@agentek/tools": "0.1.16",
    "@modelcontextprotocol/sdk": "1.7.0",
    "node-fetch": "^3.3.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "peerDependencies": {
    "viem": "^2.30.6",
    "zod": "^3.25.34"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "shx": "^0.3.4",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "ai",
    "agents",
    "blockchain",
    "mcp",
    "model-context-protocol"
  ],
  "engines": {
    "node": ">=18.17.0"
  }
}
